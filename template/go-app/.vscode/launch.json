{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Start debugging",
			"type": "go",
			"debugAdapter": "dlv-dap",
			"request": "launch",
			"mode": "exec",
			"program": "/app",
			"hideSystemGoroutines": true,
			"substitutePath": [
				{
					"from": "${workspaceFolder}",
					"to": "/src"
				},
			],
			"host": "localhost",
			"port": 50000,
		}
	]
}
